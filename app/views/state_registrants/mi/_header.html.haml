#form_header
  .state-integration
    = render partial: 'state_registrants/mi/steps'

    %h1
      = t("states.custom.mi.#{params[:step]}_header").html_safe
      .sub-header= t("states.custom.mi.#{params[:step]}_subheader", default: '').html_safe
      %p.notice= I18n.t("states.custom.mi.#{params[:step]}_notice", default: '').html_safe